SET @OLD_UNIQUE_CHECKS=@@UNIQUE_CHECKS, UNIQUE_CHECKS=0;

SET @OLD_FOREIGN_KEY_CHECKS=@@FOREIGN_KEY_CHECKS, FOREIGN_KEY_CHECKS=0;

SET @OLD_SQL_MODE=@@SQL_MODE, SQL_MODE='TRADITIONAL,ALLOW_INVALID_DATES';



CREATE SCHEMA IF NOT EXISTS `votos` DEFAULT CHARACTER SET utf8 COLLATE utf8_general_ci ;



USE `votos`;



CREATE  TABLE IF NOT EXISTS `votos`.`candidatos` (

  `id_can` INT(11) NOT NULL ,

  `nom_can` VARCHAR(100) NULL DEFAULT NULL ,

  `foto_can` VARCHAR(100) NULL DEFAULT NULL ,

  `tipo_can` VARCHAR(45) NULL DEFAULT NULL ,

  `id_grupo_can` INT(11) NULL DEFAULT NULL ,

  PRIMARY KEY (`id_can`) ,

  INDEX `id_grupo_idx` (`id_grupo_can` ASC) ,

  CONSTRAINT `id_grupo`

    FOREIGN KEY (`id_grupo_can` )

    REFERENCES `votos`.`grupo` (`idgrupo` )

    ON DELETE NO ACTION

    ON UPDATE NO ACTION)

ENGINE = InnoDB

DEFAULT CHARACTER SET = utf8

COLLATE = utf8_general_ci;



CREATE  TABLE IF NOT EXISTS `votos`.`votos` (

  `idvotos` INT(11) NOT NULL ,

  `id_candidato` INT(11) NULL DEFAULT NULL ,

  `total_voto` INT(11) NULL DEFAULT NULL ,

  PRIMARY KEY (`idvotos`) ,

  INDEX `id_can_idx` (`id_candidato` ASC) ,

  CONSTRAINT `id_can`

    FOREIGN KEY (`id_candidato` )

    REFERENCES `votos`.`candidatos` (`id_can` )

    ON DELETE NO ACTION

    ON UPDATE NO ACTION)

ENGINE = InnoDB

DEFAULT CHARACTER SET = utf8

COLLATE = utf8_general_ci;



CREATE  TABLE IF NOT EXISTS `votos`.`grado` (

  `idgrado` INT(11) NOT NULL ,

  `grado` VARCHAR(45) NULL DEFAULT NULL ,

  PRIMARY KEY (`idgrado`) )

ENGINE = InnoDB

DEFAULT CHARACTER SET = utf8

COLLATE = utf8_general_ci;



CREATE  TABLE IF NOT EXISTS `votos`.`grupo` (

  `idgrupo` INT(11) NOT NULL ,

  `grupo_nom` VARCHAR(45) NULL DEFAULT NULL ,

  `id_grado` INT(11) NULL DEFAULT NULL ,

  `cant_vot` INT(11) NULL DEFAULT NULL ,

  PRIMARY KEY (`idgrupo`) ,